{
  "hosting": {
   /* "rewrites":[
     {
     "source": "/helloWorld",
     "function": "helloWorld"
     }],*/
    "public": "dist/my-angular6",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
     "source": "/helloWorld",
     "function": "helloWorld"
     },
      {
        "source": "**",
        "destination": "/my-angular6/index.html"
      }
    ]
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ]
  }
}
